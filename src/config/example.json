{	
	"project_name": "devgrab",
	"date_time_format": "%Y%m%d-%H%M%S",

	"compression": {
		"enabled": true,

		"audio": {
			"codec": "opus",
			"bitrate_kbps": 24,
			"sample_rate": 16000
		},

		"image": {
			"codec": "jpeg",
			"jpeg_quality": 70,
			"max_width": 1920,
			"max_height": 1080
		},

		"video": {
		},

		"skip_already_compressed": true
	},

	"scheduler": {
		"enabled": true,
		"interval_minutes": 5
	}
}